/* Icons
----------------------------------*/

/* states and images */
.ui-icon { 
  display: inline-block; 
  text-indent: -99999px; 
  overflow: hidden; 
  background-repeat: no-repeat; 
  width: 16px; 
  height: 16px; 
  background-image: url(images/ui-icons-famfamfam.png); 
  vertical-align: middle;
  margin-top: -.25em;
  position: relative;
}
.ui-icon[class^="fa-"],
.ui-icon[class*="fa-"] {
  display: inline-block;
  font-size: inherit;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-indent: 0; 
  text-align:center;
  width: 16px; 
  height: 16px; 
  background: transparent;
  font-family:FontAwesome;
  font-weight:normal;
}

.ui-widget-icon-block {
  left: 50%;
  margin-left: -8px;
  display: block;
}


